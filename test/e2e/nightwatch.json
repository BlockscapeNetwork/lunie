{
  "src_folders": [
    "./test/e2e/"
  ],
  "output_folder": "./output",
  "disable_colors": false,
  "test_workers": false,
  "launch_url": "chrome-extension://cklkpejioojjeiigffappdlcmnonmjek/popup/popup.html",
  "webdriver": {
    "start_process": true,
    "port": 9515,
    "default_path_prefix": "",
    "server_path": "./node_modules/.bin/chromedriver",
    "cli_args": [
      "--verbose"
    ]
  },
  "desiredCapabilities": {
    "browserName": "chrome",
    "javascriptEnabled": true,
    "acceptSslCerts": true,
    "loggingPrefs": {
      "driver": "INFO",
      "server": "OFF",
      "browser": "INFO"
    },
    "chromeOptions": {
      "args": [
        "disable-web-security",
        "ignore-certificate-errors"
      ],
      "prefs": {
        "intl.accept_languages": "en-US,en"
      }
    }
  },
  "request_timeout_options": {
    "timeout": 60000,
    "retry_attempts": 5
  },
  "test_settings": {
    "default": {
      "silent": true,
      "filter": [
        "*.spec.js"
      ]
    }
  },
  "screenshots": {
    "enabled": true,
    "path": "./screenshots",
    "on_failure": true,
    "on_error": true
  }
}