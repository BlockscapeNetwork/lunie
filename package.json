{
  "name": "lunie-api",
  "version": "1.0.0",
  "description": "GraphQL API for Lunie.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luniehq/lunie-api.git"
  },
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "dev-cache": "cross-env NODE_ENV=development ENABLE_CACHE=true nodemon index.js",
    "dev-docker": "docker build -t lunie-api-dev . && docker run --init -it -p 4000:4000 lunie-api-dev",
    "lint": "eslint index.js config.js lib",
    "lint-fix": "yarn lint --fix",
    "start": "node index.js",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.9.0",
    "apollo-datasource-rest": "^0.6.6",
    "apollo-server": "^2.9.9",
    "apollo-server-cache-redis": "^1.1.4",
    "apollo-server-plugin-response-cache": "^0.3.5",
    "bech32": "^1.1.3",
    "bignumber.js": "^9.0.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.8.0",
    "graphql": "^14.5.8",
    "lodash": "^4.17.15",
    "websocket": "^1.0.30"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1"
  }
}
